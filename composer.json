{
  "name": "platinum/laravel-extras",
  "description": "Extend laravel applications with more artisan commands, traits and useful helper classes",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/ThePlatinum/laravel-extras#readme",
  "readme": "README.md",
  "keywords": [
    "Laravel",
    "laravel-extras",
    "make-blade",
    "make-trait",
    "trait",
    "sluggable",
    "file",
    "upload",
    "readable"
  ],
  "authors": [
    {
      "name": "Platinum",
      "email": "platinumemirate@gmail.com",
      "homepage": "https://emmannueldesina.vercel.app/",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Platinum\\LaravelExtras\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Platinum\\LaravelExtras\\LaravelExtrasServiceProvider"
      ]
    }
  },
  "require-dev": {
    "pestphp/pest": "^1.22",
    "pestphp/pest-plugin-laravel": "^1.4"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
